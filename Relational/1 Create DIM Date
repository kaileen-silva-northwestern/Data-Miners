SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[CREATE_DIM_DATE]
AS
BEGIN

CREATE TABLE DIM_Date (
[Date_Key] INT PRIMARY KEY IDENTITY (1,1)
,[Date] DATE NOT NULL
,[DayName] VARCHAR (255) NOT NULL
,[DayOfWeek] INT NOT NULL
,[Month] INT NOT NULL
,[MonthName] VARCHAR (255) NOT NULL
,[MonthYear] VARCHAR (255) NOT NULL
,[Year] INT NOT NULL
,[YearMonth] INT NOT NULL
);


END
GO
